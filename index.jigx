  - size: 1x1
    jigId: friends-list
  - size: "1x1"
    jigId: gallery

onLoad: 
  type: action.sync-entities
  options:
    provider: DATA_PROVIDER_REST
    entities:
      - entity: allActivities
        function: rest-get-activities
        functionParameters:
          accessToken: stravaoauth
