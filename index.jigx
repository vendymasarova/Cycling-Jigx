name: cycling-recording-new
title: Aim higher
category: business

home:
  - instanceId: home
    jigId: custom-home
  
widgets:
  - size: 4x2 
    jigId: user-data
  # - size: 4x2 
  #   jigId: traning-composite
  - size: "2x4"
    jigId: cycling-activities-composite
  - size: "2x2"
    jigId: run-activities-composite
  # - size: "2x2"
